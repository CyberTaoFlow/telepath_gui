title=col.title&&t.attr(col.title)?t.attr(col.title):"";title=title.replace(htmlstripre,'');paddingleft=7;width=col.width||defaultWidth;width=tmpWidth||(width-paddingleft);last=isAlreadyGrid?a.nextAll("div:eq("+(i-1)+")"):$("<div></div>").insertAfter(last);span=isAlreadyGrid?last.children("span"):$("<span></span>").appendTo(last);span.addClass(cl+" "+valClass).css({"margin-right":"0px","display":"inline-block","*display":"inline","*+display":"inline"}).html(content).click(cellClickHandler(val,col,s));last=last.css(conf).css({width:width,"padding-left":paddingleft+"px"}).addClass("jstree-grid-cell jstree-grid-cell-"+classAdd+" "+wcl+" "+wideValClass+(tr?" ui-state-default":"")).addClass("jstree-grid-col-"+i);if(title){span.attr("title",title);}}
last.addClass("jstree-grid-cell-last"+(tr?" ui-state-default":""));$("<div></div>").css("clear","both").insertAfter(last);}
this.element.css({'overflow-y':'auto !important'});};};}(jQuery));